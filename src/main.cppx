#include <raylib.h>

int main(void) {
  InitWindow(1200, 800, "hello");

  while (!WindowShouldClose())  // Detect window close button or ESC key
  {
    BeginDrawing();

    ClearBackground(GRAY);

    DrawText("Hello world", 190, 200, 50, BLUE);
    DrawCircle(100, 100, 50, RED);
    DrawRectangle(100, 200, 50, 50, GREEN);

    EndDrawing();
  }

  CloseWindow();

  return 0;
}